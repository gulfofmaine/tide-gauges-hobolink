<!-- BEGIN ONSET_scituate_hourly_water_level -->
<dataset type="EDDTableFromAsciiFiles" datasetID="ONSET_scituate_hourly_water_level" active="true">
    <reloadEveryNMinutes>60</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/home2/data/ONSET/</fileDir>
    <fileNameRegex>.*_scituate_hourly\.csv</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <charset>ISO-8859-1</charset>
    <columnSeparator>,</columnSeparator>
    <columnNamesRow>1</columnNamesRow>
    <firstDataRow>2</firstDataRow>
    <preExtractRegex></preExtractRegex>
    <postExtractRegex></postExtractRegex>
    <extractRegex></extractRegex>
    <columnNameForExtract></columnNameForExtract>
    <sortedColumnSourceName>Time</sortedColumnSourceName>
    <sortFilesBySourceNames>Time</sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <accessibleViaFiles>false</accessibleViaFiles>
    <!-- sourceAttributes>
    </sourceAttributes -->
    <!-- Please specify the actual cdm_data_type (TimeSeries?) and related info below, for example...
        <att name="cdm_timeseries_variables">station, longitude, latitude</att>
        <att name="subsetVariables">station, longitude, latitude</att>
    -->
    <addAttributes>
        <att name="cdm_data_type">Point</att>
        <att name="Conventions">COARDS, CF-1.6, ACDD-1.3</att>
        <att name="creator_name">Onset Hobolink</att>
        <att name="creator_type">institution</att>
        <att name="creator_url">http://hobolink.com</att>
        <att name="creator_email">Francesco.Peri@umb.edu</att>
        <att name="infoUrl">http://hobolink.com</att>
        <att name="institution">Onset Hobolink</att>
        <att name="project">NERACOOS</att>
        <att name="project_url" >http://www.neracoos.org</att>
        <att name="publisher_email" >info@neracoos.org</att>
        <att name="publisher_url" >http://www.neracoos.org</att>
        <att name="publisher_phone" >(603) 319 1785</att>
        <att name="keywords">gauge, water level, scituate, hobolink, mean, meter, mllw, MLLW_feet, MLLW_meter, msl, onset, sea, station, Temp, temperature, tide</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v29</att>
        <att name="subsetVariables"></att>
        <att name="summary">Tide gauge located in Scituate Harbor, MA. Funded by NERACOOS.  Data is reported in elevation above MLLW (NAVD88) in feet and also converted to meters.  NOAA uses Mean Lower Low Water (MLLW), which is the average height of the lowest tide recorded at a tide station each day during a 19-year recording period, known as the National Tidal Datum Epoch.[5] MLLW is only a mean, so some tidal levels may be negative relative to MLLW; see also mean low water spring. The 19-year recording period is the nearest full year count to the 18.6-year cycle of the lunar node regression, which has an effect on tides.  At this location Mean Sea Level (MSL) is 1.491 meters above MLLW Benchmark used: NOAA Station ID: 8445138   - SCITUATE, SCITUATE HARBOR (with extrapolated estimate) MSL meters = (MLLW ft. * 0.3048) + GUAGE location correction.</att>
        <att name="testOutOfDate">now-1day</att>
        <att name="title">Scituate Harbor, MA Tide Gauge</att>
    </addAttributes>
    <dataVariable>
        <sourceName>Station</sourceName>
        <destinationName>Station</destinationName>
        <dataType>String</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
