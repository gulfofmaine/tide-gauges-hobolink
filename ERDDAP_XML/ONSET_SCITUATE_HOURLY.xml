            <att name="units">yyyy-MM-dd&#39;T&#39;HH:mm:ss</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>MLLW_feet</sourceName>
        <destinationName>MLLW_feet</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="ioos_category">Unknown</att>
